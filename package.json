{
  "name": "aura-match-root",
  "private": true,
  "workspaces": [
    "client",
    "server",
    "shared"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=@aura-match/client\" \"npm run dev --workspace=server\"",
    "build": "npm run build --workspaces",
    "start": "npm start --workspace=server"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "typescript": "^5.0.0"
  }
}
